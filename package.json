  "description": "Collection of low level Javascript helper functions targeting language primitives and native objects",
  "main": "lib/index.js",
  "scripts": {
    "test": "find ./lib -name 'test.js' | xargs ./node_modules/mocha/bin/mocha",
    "testone": "./node_modules/mocha/bin/mocha",
