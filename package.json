{
  "name": "@fiverr/commodity",
  "version": "1.1.0",
  "author": "Fiverr",
  "license": "MIT",
  "description": "Collection of low level Javascript helper functions targeting language primitives and native objects",
  "engines": {
    "node" : ">=6.9.4",
    "npm" : "~3.10.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fiverr/commodity.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "find ./lib -name 'test.js' | xargs ./node_modules/mocha/bin/mocha",
    "testone": "./node_modules/mocha/bin/mocha",
    "lint": "npm run lint-code && npm run lint-tests",
    "lint-code": "./node_modules/eslint/bin/eslint.js -c .eslintrc ./lib/**/* --ignore-path .eslintignore --quiet",
    "lint-tests": "./node_modules/eslint/bin/eslint.js -c .eslintrc-tests ./lib/**/* --ignore-path .eslintignore-tests --quiet",
    "doc": "rm -rf docs/* && ./node_modules/jsdoc/jsdoc.js lib/ -c .jsdocrc.json --readme lib/doc.md",
    "publish": "./scripts/publish.sh"
  },
  "devDependencies": {
    "@fiverr/eslint-config-fiverr": "0.0.9",
    "chai": "^3.5.0",
    "docdash": "^0.4.0",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0"
  },
  "dependencies": {}
}
